    "primevue": "^4.2.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.1.1",
    "@nuxt/eslint-config": "^1.1.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.17.0",
    "@types/node": "latest",
    "changelogen": "^0.6.0",
    "eslint": "^9.21.0",
    "nuxt": "^3.15.4",
    "typescript": "~5.8.2",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.4"
